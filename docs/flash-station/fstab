config mount
	option target	/home
	option device	/dev/sda1
	option fstype	ext3
	option options	rw,sync
	option enabled	0

config swap
	option device	/dev/sda2
	option enabled	0
	
config mount
	option target	/tmp/images
	option device	192.168.1.2:/srv/nfs/images
	option fstype	nfs
	option options	rsize=32768,wsize=32768,hard,udp,nolock
	option enabled	1
